{
  "name": "tapedeck-client",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "prod": "npm run build && npx serve -s dist",
    "lint": "vue-cli-service lint",
    "test": "npm run serve && npx wait-on http://localhost:8080 && npx cypress run",
    "kill": "pgrep -f \"vue-cli-service\" | xargs kill"
  },
  "dependencies": {
    "@aws-amplify/auth": "^3.4.18",
    "@aws-amplify/core": "^3.8.10",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "vue": "^3.0.0",
    "vue-router": "^4.0.6",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "cypress": "^6.3.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "prettier": "^2.2.1",
    "serve": "^11.3.2",
    "wait-on": "^5.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es2017": true,
      "browser": true,
      "node": true,
      "cypress/globals": true
    },
    "plugins": [
      "vue",
      "prettier",
      "cypress"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-essential",
      "@vue/prettier",
      "prettier",
      "plugin:cypress/recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "strict": "error",
      "semi": "error",
      "prettier/prettier": "error",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    },
    "ignorePatterns": [
      "node_modules/",
      "dist/",
      "src/old/"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "quoteProps": "consistent"
  }
}
